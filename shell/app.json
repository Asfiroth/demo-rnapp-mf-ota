{
  "expo": {
    "name": "shell",
    "slug": "shell",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "demo",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": "exposdk:52.0.25",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/0c82d040-7f36-454e-bfc7-219dca8c4204"
    },
    "extra": {
      "eas": {
        "projectId": "0c82d040-7f36-454e-bfc7-219dca8c4204"
      }
    },
    "owner": "asfiroth",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asfiroth.shell"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asfiroth.shell"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
